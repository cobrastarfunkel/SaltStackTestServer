default menu.c32
promtp 0
timeout 300
ONTIMEOUT local


menu title ##### PXE Boot Menu #####

label 1
MENU LABEL Arch Linux x86_64
LINUX /arch/boot/x86_64/vmlinuz
INITRD /arch/boot/intel_ucode.img,/arch/boot/amd_ucode.img,/arch/boot/x86_64/archiso.img
SYSAPPEND 2

label 2
MENU LABEL CentOS 7
KERNEL /centos/vmlinuz
APPEND initrd=/centos/initrd.img inst.repo=ftp://192.168.56.2/pub/ ks=ftp://192.168.56.2/pub/centos7.ks
